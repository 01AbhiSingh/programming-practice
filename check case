#include <bits/stdc++.h>

using namespace std;

int main()

{

    ios_base::sync_with_stdio(false);

    cin.tie(NULL);

    char ch;

    cin>>ch;

    if(ch>='A' && ch<='Z'){

        cout<<1<<endl;

    }else if(ch>='a' && ch<='z'){

        cout<<0<<endl;

    }else{

        cout<<-1<<endl;

    }

    return 0;

}
